{
  "server": {
    "port": 3000,
    "host": "localhost",
    "cors": {
      "origin": ["http://localhost:3000", "http://localhost:3001"],
      "credentials": false
    }
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "name": "sen3ai",
    "username": "postgres",
    "password": ""
  },
  "blockchains": [
    {
      "name": "ethereum",
      "regex": "^0x[0-9a-fA-F]{40}$",
      "enabled": true
    },
    {
      "name": "polygon",
      "regex": "^0x[0-9a-fA-F]{40}$",
      "enabled": true
    },
    {
      "name": "bsc",
      "regex": "^0x[0-9a-fA-F]{40}$",
      "enabled": true
    },
    {
      "name": "arbitrum",
      "regex": "^0x[0-9a-fA-F]{40}$",
      "enabled": true
    },
    {
      "name": "optimism",
      "regex": "^0x[0-9a-fA-F]{40}$",
      "enabled": true
    },
    {
      "name": "solana",
      "regex": "^[1-9A-HJ-NP-Za-km-z]{32,44}$",
      "enabled": true
    },
    {
      "name": "bitcoin",
      "regex": "^[13][a-km-zA-HJ-NP-Z1-9]{25,34}$",
      "enabled": false
    }
  ],
  "providers": [
    {
      "name": "blockchain",
      "enabled": true,
      "priority": 1,
      "timeout": 5000,
      "retries": 3
    },
    {
      "name": "reputation",
      "enabled": true,
      "priority": 2,
      "timeout": 3000,
      "retries": 2
    },
    {
      "name": "social",
      "enabled": true,
      "priority": 3,
      "timeout": 4000,
      "retries": 2
    },
    {
      "name": "onchain",
      "enabled": true,
      "priority": 4,
      "timeout": 6000,
      "retries": 3
    },
    {
      "name": "defi",
      "enabled": false,
      "priority": 5,
      "timeout": 8000,
      "retries": 2
    },
    {
      "name": "nft",
      "enabled": false,
      "priority": 6,
      "timeout": 4000,
      "retries": 2
    }
  ],
  "processors": [
    {
      "name": "comprehensive",
      "enabled": true,
      "weight": 1.0
    },
    {
      "name": "ml",
      "enabled": false,
      "weight": 0.8
    },
    {
      "name": "heuristic",
      "enabled": false,
      "weight": 0.6
    }
  ],
  "logLevel": "info",
  "features": {
    "caching": true,
    "rateLimiting": true,
    "metrics": true,
    "healthChecks": true
  },
  "limits": {
    "maxAddressesPerRequest": 10,
    "maxConcurrentRequests": 100,
    "requestTimeout": 30000
  }
} 